import java.util.Scanner;

public class NextNumber {

	public static void main(){

		Scanner scan = new Scanner(System.in);
		int[] array;
		System.err.print("Mata in n: ");
		int n = scan.nextInt();
		array = new int[n];
		boolean check = false;

		for (int i=0; n>i ;i++) {
			int s = scan.nextInt();
			array[i] = s;
		}
		
		for (int i=1; n>i ;i++) {
			System.out.println(array[i]);
			if(!check){
				for (int c=-9; c<=9; c++){
					for(int b=-9; b<=9; b++){
						if (array[i]*b+c==array[i+1]){
							check = true;
						}
					}
				}			
			}
		}
	}
}